{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./main/bin/www",
    "codecov-runner": "cross-env NODE_ENV=test jest --runInBand --coverage --detectOpenHandles --forceExit ./test", 
    "test": "cross-env NODE_ENV=test jest --runInBand --coverage --detectOpenHandles --forceExit ./test && eval \"$(grep ^CODECOV_TOKEN= .env.test)\" && codecov -t $CODECOV_TOKEN"
  },
  "jest": {
    "bail": true,
    "testEnvironment": "node"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "custom-env": "^2.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.23",
    "morgan": "~1.9.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "codecov": "^3.8.0",
    "jest": "^26.5.2",
    "mongodb-memory-server": "^6.9.1",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0"
  }
}
